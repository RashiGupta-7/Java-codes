package com;

import java.util.Scanner;

public class rashi {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        System.out.println("Enter number: ");
        int n = input.nextInt();
        int rev=0;
        while (n > 0) {
            int temp = n % 10;
            n = n / 10;
            rev = rev * 10 + temp;
        }
        System.out.println(rev);
    }
}

package com;

import java.util.Scanner;

public class rashi {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        System.out.println("Enter number: ");
        int n = input.nextInt();
        int a = 0;
        int b = 1;
        int temp;
        System.out.println(a);
        System.out.println(b);
        for (int i = 2; i < n; i++) {
            temp = a+b;
            System.out.println(temp);
            a=b;
            b=temp;
        }
    }
}

package com;

import java.util.Scanner;

class Pattern {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int n = input.nextInt();
        for (int i = n; i >= 1; i--) // Outer loop
        {
            for (int j = i; j >= 1; j--) //Inner loop
            {
                System.out.print("*");
            }
            System.out.println();
        }
    }
}
